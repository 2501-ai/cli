    "type": "curl",
    "commands": [
      {
        "description": "Create a new AI agent",
        "url": "https://engine.2501.ai/api/v1/agents",
        "method": "POST",
        "auth": "Bearer {2501_bearer_token}",
        "body": {
          "workspace": "string",
          "configuration": "string",
          "prompt": "string",
          "engine": "string"
        }
      }
    ]
  },
  "rtms": {
    "type": "curl",
    "commands": [
      {
        "description": "Send a notification",
        "url": "http://localhost:3000/api/notify",
        "method": "POST",
        "auth": "Bearer {2501_rtms_token}",
        "body": {
          "ticketid": "string",
          "message": "string",
          "status": "string",
          "source": "string"
        }
      }
    ]
  }
