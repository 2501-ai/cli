    "@types/jest": "^29.5.12",
    "@types/marked-terminal": "^6.1.1",
    "@types/terminal-kit": "^2.5.6",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
